{
  "name": "frontend_visa_center",
  "private": true,
  "version": "0.0.0",
  "type": "commonjs",
  "scripts": {
    "dev": "vite -m dev",
    "build": "dotenv -e .env.prod -- tsc && vite build -m prod",
    "lint": "eslint src --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "preview": "vite preview -m dev",
    "codegen": "dotenv -e .env.dev -- graphql-codegen --config codegen/configs/codegen-hooks.ts",
    "codegen:schema": "dotenv -e .env.dev -- graphql-codegen --config codegen/configs/codegen-schema-loader.ts",
    "codegen:full": "dotenv -e .env.dev -- npm run codegen:schema && npm run codegen"
  },
  "dependencies": {
    "@apollo/client": "^3.7.17",
    "@emotion/react": "^11.11.1",
    "@emotion/styled": "^11.11.0",
    "@mui/icons-material": "^5.14.0",
    "@mui/material": "^5.14.0",
    "@mui/styled-engine-sc": "^5.12.0",
    "@zxcvbn-ts/core": "^3.0.2",
    "@zxcvbn-ts/language-common": "^3.0.3",
    "apollo-link-scalars": "^4.0.2",
    "dayjs": "^1.11.9",
    "formik": "^2.4.2",
    "graphql": "^16.7.1",
    "graphql-ws": "^5.14.0",
    "lodash": "^4.17.21",
    "ms": "^2.1.3",
    "qrcode.react": "^3.1.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-icons": "^4.10.1",
    "react-router-dom": "^6.14.1",
    "react-toastify": "^9.1.3",
    "styled-components": "^5.3.11",
    "yup": "^1.2.0",
    "zustand": "^4.3.9",
    "@react-buddy/ide-toolbox": "^2.4.0",
    "@react-buddy/palette-mui": "^5.0.2"
  },
  "devDependencies": {
    "@graphql-codegen/add": "^5.0.0",
    "@graphql-codegen/cli": "^4.0.1",
    "@graphql-codegen/introspection": "^4.0.0",
    "@graphql-codegen/schema-ast": "^4.0.0",
    "@graphql-codegen/typescript": "^4.0.1",
    "@graphql-codegen/typescript-operations": "^4.0.1",
    "@graphql-codegen/typescript-react-apollo": "^3.3.7",
    "@types/lodash": "^4.14.195",
    "@types/ms": "^0.7.31",
    "@types/node": "^20.4.2",
    "@types/react": "^18.2.15",
    "@types/react-dom": "^18.2.7",
