{
  "name": "frontend_visa_center",
  "private": true,
  "version": "0.0.0",
  "type": "commonjs",
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "lint": "eslint src --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "preview": "vite preview",
    "codegen": "graphql-codegen --config codegen.ts",
    "codegen:schema": "graphql-codegen --config codegen-schema-loader.ts",
    "codegen:full": "npm run codegen:schema && npm run codegen",
    "zeus:generate": "zeus ./src/api/schema.gql src/api/zeus --typescript --apollo --documentNode",
    "zeus:schema": "dotenv -- cross-var zeus %VITE_API_GRAPHQL_ENDPOINT% --graphql ./src/api/zeus/schema.gql"
  },
  "dependencies": {
    "@apollo/client": "^3.7.16",
    "@emotion/react": "^11.11.1",
    "@emotion/styled": "^11.11.0",
    "@mui/icons-material": "^5.13.7",
    "@mui/material": "^5.13.7",
    "@mui/styled-engine-sc": "^5.12.0",
    "@zxcvbn-ts/core": "^3.0.2",
    "@zxcvbn-ts/language-common": "^3.0.3",
    "apollo-link-scalars": "^4.0.2",
    "dayjs": "^1.11.9",
    "dotenv-cli": "^7.2.1",
    "formik": "^2.4.2",
    "graphql": "^16.7.1",
    "lodash": "^4.17.21",
    "ms": "^2.1.3",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-icons": "^4.10.1",
    "react-router-dom": "^6.14.1",
    "react-toastify": "^9.1.3",
    "styled-components": "^6.0.2",
    "yup": "^1.2.0",
    "zustand": "^4.3.9"
  },
  "devDependencies": {
    "@graphql-codegen/add": "^5.0.0",
    "@graphql-codegen/cli": "^4.0.1",
    "@graphql-codegen/introspection": "^4.0.0",
    "@graphql-codegen/schema-ast": "^4.0.0",
    "@graphql-codegen/typescript": "^4.0.1",
    "@graphql-codegen/typescript-operations": "^4.0.1",
    "@graphql-codegen/typescript-react-apollo": "^3.3.7",
    "@types/lodash": "^4.14.195",
    "@types/ms": "^0.7.31",
    "@types/react": "^18.2.14",
    "@types/react-dom": "^18.2.6",
    "@types/zxcvbn": "^4.4.1",
    "@typescript-eslint/eslint-plugin": "^5.61.0",
    "@typescript-eslint/parser": "^5.61.0",
    "@vitejs/plugin-react": "^4.0.1",
    "@vitejs/plugin-react-swc": "^3.3.2",
    "autoprefixer": "^10.4.14",
    "cross-var": "^1.1.0",
    "eslint": "^8.44.0",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-config-airbnb-typescript": "^17.0.0",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-jsx-a11y": "^6.7.1",
    "eslint-plugin-react": "^7.32.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.4.1",
    "postcss": "^8.4.24",
    "sass": "^1.63.6",
    "tailwindcss": "^3.3.2",
    "type-fest": "^3.12.0",
    "typescript": "^5.1.6",
    "vite": "^4.3.9"
  }
}
