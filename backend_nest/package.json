  "scripts": {
    "prebuild": "rimraf dist",
    "build": "nest build",
    "start:dev": "dotenv -e .env.dev -- nest start --watch",
    "start:debug": "dotenv -e .env.dev -- nest start --debug --watch",
    "start:prod": "dotenv -e .env.prod -- node dist/main",
    "swc:build": "concurrently \"npx swc --out-dir dist -w src\" \"ts-node-dev watch-assets.ts\"",
    "swc:assets": "ts-node-dev watch-assets.ts",
    "swc:start": "dotenv -e .env.dev -- nodemon --delay .2 dist/main",
    "swc:start2": "dotenv -e .env.dev -- ts-node-dev --respawn dist/main",
    "prisma:dev:migrate": "dotenv -e .env.dev -- prisma migrate dev",
    "prisma:format": "prisma format",
    "prisma:generate": "prisma generate",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix"
  },
